<p id="notice"><%= notice %></p>

<h1>Membership Fees</h1>

<table>
  <thead>
    <tr>
      <th>Membership fees status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @membership_fees.each do |membership_fee| %>
      <tr>
        <td><%= membership_fee.membership_fees_status %></td>
        <td><%= link_to 'Show', membership_fee %></td>
        <td><%= link_to 'Edit', edit_membership_fee_path(membership_fee) %></td>
        <td><%= link_to 'Destroy', membership_fee, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Membership Fee', new_membership_fee_path %>
