<p id="notice"><%= notice %></p>

<h1>Members</h1>

<table>
  <thead>
    <tr>
      <th>Member no</th>
      <th>Title</th>
      <th>First name</th>
      <th>Middle initial</th>
      <th>Last name string</th>
      <th>Birthdate</th>
      <th>Age</th>
      <th>Gender</th>
      <th>Street address</th>
      <th>City</th>
      <th>County</th>
      <th>State</th>
      <th>Postal code</th>
      <th>Email</th>
      <th>Home phone</th>
      <th>Cell phone</th>
      <th>Send card</th>
      <th>Plan</th>
      <th>Membership fee</th>
      <th>Role</th>
      <th>Status</th>
      <th>Member since</th>
      <th>Membership ends</th>
      <th>Notes</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.member_no %></td>
        <td><%= member.title %></td>
        <td><%= member.first_name %></td>
        <td><%= member.middle_initial %></td>
        <td><%= member.last_name_string %></td>
        <td><%= member.birthdate %></td>
        <td><%= member.age %></td>
        <td><%= member.gender.gender %></td>
        <td><%= member.street_address %></td>
        <td><%= member.city %></td>
        <td><%= member.county.county %></td>
        <td><%= member.state.stateCode %></td>
        <td><%= member.postal_code %></td>
        <td><%= member.email %></td>
        <td><%= member.home_phone %></td>
        <td><%= member.cell_phone %></td>
        <td><%= member.send_card %></td>
        <td><%= member.plan.Membership_plan %></td>
        <td><%= member.membership_fee.membership_fees_status %></td>
        <td><%= member.role.role %></td>
        <td><%= member.status.status %></td>
        <td><%= member.member_since %></td>
        <td><%= member.membership_ends %></td>
        <td><%= member.notes %></td>
        <td><%= link_to 'Show', member %></td>
        <td><%= link_to 'Edit', edit_member_path(member) %></td>
        <td><%= link_to 'Destroy', member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Member', new_member_path %>
